{
  "name": "HHS USWDS 3.0 Theme",
  "description": "Official HHS Power BI theme aligned with USWDS 3.0 Design System",
  "version": "2.0.0",
  "author": "HHS ASPA WebFirst Team",
  "dataColors": [
    "#005EA2",
    "#0076D6",
    "#00BDE3",
    "#FA9441",
    "#E52207",
    "#B50909",
    "#1B1B1B",
    "#71767A"
  ],
  "foreground": "#1B1B1B",
  "foregroundNeutralSecondary": "#565C65",
  "foregroundNeutralTertiary": "#71767A",
  "background": "#FFFFFF",
  "backgroundLight": "#F7FAFC",
  "backgroundNeutral": "#F0F0F0",
  "tableAccent": "#005EA2",
  "good": "#00A91C",
  "neutral": "#FFBE2E",
  "bad": "#D54309",
  "maximum": "#005EA2",
  "center": "#FFBE2E",
  "minimum": "#D54309",
  "textClasses": {
    "title": {
      "fontFace": "Source Sans Pro",
      "fontSize": "16pt",
      "fontWeight": "bold",
      "color": "#1B1B1B"
    },
    "header": {
      "fontFace": "Source Sans Pro",
      "fontSize": "14pt",
      "fontWeight": "bold",
      "color": "#1B1B1B"
    },
    "callout": {
      "fontFace": "Source Sans Pro",
      "fontSize": "24pt",
      "fontWeight": "bold",
      "color": "#005EA2"
    },
    "label": {
      "fontFace": "Source Sans Pro",
      "fontSize": "10pt",
      "fontWeight": "normal",
      "color": "#565C65"
    },
    "smallLabel": {
      "fontFace": "Source Sans Pro",
      "fontSize": "8pt",
      "fontWeight": "normal",
      "color": "#71767A"
    }
  },
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": true,
            "color": { "solid": { "color": "#D6D9DC" } },
            "radius": 0
          }
        ],
        "title": [
          {
            "show": true,
            "fontColor": { "solid": { "color": "#1B1B1B" } },
            "fontSize": 12,
            "fontFamily": "Source Sans Pro",
            "bold": true
          }
        ],
        "dropShadow": [
          {
            "show": false
          }
        ],
        "visualHeader": [
          {
            "show": false
          }
        ],
        "visualHeaderTooltip": [
          {
            "show": true
          }
        ],
        "padding": [
          {
            "top": 8,
            "right": 8,
            "bottom": 8,
            "left": 8
          }
        ]
      }
    },
    "card": {
      "*": {
        "categoryLabels": [
          {
            "show": true,
            "color": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#005EA2" } },
            "fontSize": 28,
            "fontFamily": "Source Sans Pro",
            "bold": true
          }
        ]
      }
    },
    "multiRowCard": {
      "*": {
        "dataLabels": [
          {
            "color": { "solid": { "color": "#005EA2" } },
            "fontSize": 20,
            "fontFamily": "Source Sans Pro",
            "bold": true
          }
        ],
        "categoryLabels": [
          {
            "color": { "solid": { "color": "#565C65" } },
            "fontSize": 9,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "cardTitle": [
          {
            "color": { "solid": { "color": "#1B1B1B" } },
            "fontSize": 11,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "card": [
          {
            "outline": "None",
            "barShow": true,
            "barColor": { "solid": { "color": "#005EA2" } },
            "barWeight": 3
          }
        ]
      }
    },
    "lineChart": {
      "*": {
        "lineStyles": [
          {
            "strokeWidth": 2
          }
        ],
        "categoryAxis": [
          {
            "labelColor": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "valueAxis": [
          {
            "labelColor": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro",
            "gridlineColor": { "solid": { "color": "#E6E6E6" } },
            "gridlineStyle": "solid"
          }
        ],
        "legend": [
          {
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ]
      }
    },
    "barChart": {
      "*": {
        "categoryAxis": [
          {
            "labelColor": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "valueAxis": [
          {
            "labelColor": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro",
            "gridlineColor": { "solid": { "color": "#E6E6E6" } }
          }
        ],
        "labels": [
          {
            "fontSize": 9,
            "fontFamily": "Source Sans Pro"
          }
        ]
      }
    },
    "clusteredColumnChart": {
      "*": {
        "categoryAxis": [
          {
            "labelColor": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "valueAxis": [
          {
            "labelColor": { "solid": { "color": "#565C65" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro",
            "gridlineColor": { "solid": { "color": "#E6E6E6" } }
          }
        ]
      }
    },
    "tableEx": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": { "solid": { "color": "#E6E6E6" } },
            "gridHorizontal": true,
            "gridHorizontalColor": { "solid": { "color": "#E6E6E6" } },
            "rowPadding": 4
          }
        ],
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#FFFFFF" } },
            "backColor": { "solid": { "color": "#005EA2" } },
            "fontSize": 11,
            "fontFamily": "Source Sans Pro",
            "bold": true
          }
        ],
        "values": [
          {
            "fontColor": { "solid": { "color": "#1B1B1B" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "total": [
          {
            "fontColor": { "solid": { "color": "#1B1B1B" } },
            "backColor": { "solid": { "color": "#F0F0F0" } },
            "bold": true
          }
        ]
      }
    },
    "pivotTable": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": { "solid": { "color": "#E6E6E6" } },
            "gridHorizontal": true,
            "gridHorizontalColor": { "solid": { "color": "#E6E6E6" } }
          }
        ],
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#FFFFFF" } },
            "backColor": { "solid": { "color": "#005EA2" } },
            "fontSize": 11,
            "fontFamily": "Source Sans Pro"
          }
        ],
        "rowHeaders": [
          {
            "fontColor": { "solid": { "color": "#1B1B1B" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro"
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "general": [
          {
            "outlineColor": { "solid": { "color": "#D6D9DC" } },
            "outlineWeight": 1
          }
        ],
        "header": [
          {
            "fontColor": { "solid": { "color": "#1B1B1B" } },
            "fontSize": 11,
            "fontFamily": "Source Sans Pro",
            "bold": true
          }
        ],
        "items": [
          {
            "fontColor": { "solid": { "color": "#1B1B1B" } },
            "fontSize": 10,
            "fontFamily": "Source Sans Pro",
            "background": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "selection": [
          {
            "selectAllCheckboxEnabled": true,
            "singleSelect": false
          }
        ]
      }
    }
  }
}














