  "semanticModel": {
    "name": "HHS Live Events Performance Dashboard",
    "tables": {
      "fact": ["ga4-daily", "ga4-daily2", "ga4-events", "ga4-pages"],
      "dimension": ["DimDate", "DimLivecast", "DimDevice", "DimGeography", "DimSource", "DimAction"],
      "measures": "Measures_Livecast"
    }
  },

