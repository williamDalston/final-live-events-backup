  "name": "HHS Live Events Performance Dashboard",
  "version": "2.0",
  "description": "7-page Live Events Dashboard with Actions Panel - 1920x1080 canvas, USWDS 3.0 compliant",

  "canvas": {
    "width": 1920,
    "height": 1080,
    "unit": "px"
  },

  "theme": {
    "file": "../../05_Themes/HHS_USWDS_Theme.json",
    "colors": {
      "primary": "#005EA2",
      "primaryVivid": "#0076D6",
      "primaryDark": "#1A4480",
      "primaryDarker": "#162E51",
      "background": "#F0F0F0",
      "white": "#FFFFFF",
      "text": "#1B1B1B",
      "textSecondary": "#565C65",
      "textTertiary": "#71767A",
      "border": "#DFE1E2",
      "innerPlaceholder": "#F8F9FA",
      "hoverFill": "#E7F2F4",
      "hoverBorder": "#0D5C6D",
      "success": "#00A91C",
      "error": "#D83933",
      "warning": "#FFBE2E",
      "highPriority": "#D83B01",
      "mediumPriority": "#E5A000",
      "infoPriority": "#0076D6"
    }
  },

  "defaults": {
    "safeMargin": 84,
    "navRailWidth": 60,
    "gridAlignment": 12,
    "cardWidth": 295,
    "cardHeight": 100,
    "borderRadius": 4,
    "borderColor": "#DFE1E2",
    "fontFamily": "Segoe UI"
  },

  "layout": {
    "nav": {"x": 0, "y": 0, "w": 60, "h": 1080, "background": "#FFFFFF"},
